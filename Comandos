-- Comando para ver puerto y mac de lo que tiene conectado un switch

snmpwalk -v2c -c ffr 10.1.0.10 1.3.6.1.2.1.17.4.3.1.2 && snmpwalk -v2c -c ffr 10.1.0.10 1.3.6.1.2.1.17.4.3.1.1

-- Informacion del sistema.
snmpwalk -v2c -c ffr 10.1.0.10 1.3.6.1.2.1.1

-- Mac e IP de lo conectado. 
snmpwalk -v2c -c ffr 10.1.0.10 1.3.6.1.2.1.3

-- Mucha info sobre IP.
snmpwalk -v2c -c ffr 10.1.0.10 1.3.6.1.2.1.4

-- Entre .4 y .11 info de distintos protocolos.

-- Mucha info sobre SNMP. 
snmpwalk -v2c -c ffr 10.1.0.10 1.3.6.1.2.1.11


